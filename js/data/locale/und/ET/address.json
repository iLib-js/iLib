{
    "formats": {
        "default": "{streetAddress}\n{postalCode} {locality}\n{country}",
        "nocountry": "{streetAddress}\n{postalCode} {locality}"
    },
    "startAt": "end",
    "fields": [
        
	{
            "name": "locality",
            "line": "startAtLast",
            "pattern": "(([A-z]+)? ?[A-z]+)$",
            "matchGroup": 1
        },
	{
            "name": "postalCode",
            "line": "last",
            "pattern": "([0-9]{4})$",
            "matchGroup": 0
        }
	
      
        
    ]
}
